<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <!--Route-路由，IsMust-是否只能是MVC方式，0-否，1-是-->
  <BusinessRoute Route="Login|Index" IsMust="1"></BusinessRoute>
  <BusinessRoute Route="Login|Logout" IsMust="1"></BusinessRoute>
  <BusinessRoute Route="SystemUser|Login" IsMust="1"></BusinessRoute>
</configuration>
