<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <!--persistType为Cookie实现类型，有Web、Mobile等值；securityLevel为安全等级，有Low、Middle、High等值-->
  <cookies nodeName="TestCookie" persistType="Web" securityLevel="High">
    <!--Cookie名称-->
    <cookieName><![CDATA[TestCookie]]></cookieName>
    <!--cookie值存放编码方式，有json和normal方式，normal为明文字符串方式-->
    <encode>json</encode>
    <!--Hash密钥-->
    <hashkey><![CDATA[baeaaea5-3d57-4b98-abde-47ac0aa15d54]]></hashkey>
    <!--RC4密钥-->
    <rc4key><![CDATA[5cb8b18c-7b5e-4f7b-a7c2-4603a250f39b]]></rc4key>
    <!--所在域名-->
    <domain><![CDATA[localhost]]></domain>
    <!--存放路径-->
    <path><![CDATA[/]]></path>
    <!--有效期，配置为0，则Cookie在会话结束时失效，单位为分钟-->
    <expires><![CDATA[0]]></expires>
    <!--Cookie值有效期，单位为分钟-->
    <securityExpires><![CDATA[1]]></securityExpires>
  </cookies>
  <!--登录Cookie-->
  <cookies nodeName="LoginCookie" persistType="Auto" securityLevel="Middle">
    <cookieName><![CDATA[soho.web.auth]]></cookieName>
    <encode>normal</encode>
    <hashkey><![CDATA[baeaaea5-3d57-4b98-abde-47ac0aa15d54]]></hashkey>
    <rc4key><![CDATA[5cb8b18c-7b5e-4f7b-a7c2-4603a250f39b]]></rc4key>
    <domain><![CDATA[localhost]]></domain>
    <path><![CDATA[/]]></path>
    <expires><![CDATA[0]]></expires>
    <securityExpires><![CDATA[1]]></securityExpires>
  </cookies>
  <!--用户名Cookie-->
  <cookies nodeName="UserNameCookie" persistType="Auto" securityLevel="Low">
    <cookieName><![CDATA[soho.web.username]]></cookieName>
    <encode>json</encode>
    <hashkey><![CDATA[baeaaea5-3d57-4b98-abde-47ac0aa15d54]]></hashkey>
    <rc4key><![CDATA[5cb8b18c-7b5e-4f7b-a7c2-4603a250f39b]]></rc4key>
    <domain><![CDATA[localhost]]></domain>
    <path><![CDATA[/]]></path>
    <expires><![CDATA[0]]></expires>
    <securityExpires><![CDATA[1]]></securityExpires>
  </cookies>
</configuration>
