<?xml version="1.0" encoding="utf-8" ?>
<dataOperations xmlns="http://www.common.com/DataOperation">
  <!--示例-->
  <dataCommand name="Demo" database="MainDB" commandType="Text">
    <commandText>
      <![CDATA[
      SELECT SysNo,UserID,UserPwd
	      FROM Analysis28DB.dbo.Users(NOLOCK)
	      WHERE SysNo > @SysNo
      ]]>
    </commandText>
    <parameters>
      <param name="@SysNo" dbType="Int32" />
    </parameters>
  </dataCommand>
</dataOperations>
